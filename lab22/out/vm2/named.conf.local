zone "lab22.stosun.org" {
	type slave;
	file "/etc/bind/zones/db.lab22.stosun.org";
	masters { 192.168.220.101; };
};

zone "220.168.192.in-addr.arpa" {
	type slave;
	file "/etc/bind/zones/db.192";
	masters { 192.168.220.101; };
};


